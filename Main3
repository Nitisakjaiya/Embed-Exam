#define IN1 18
#define IN2 19
#define IN3 22
#define IN4 23

void setup() {
  pinMode(IN1, OUTPUT);
  pinMode(IN2, OUTPUT);
  pinMode(IN3, OUTPUT);
  pinMode(IN4, OUTPUT);
}

void loop() {

  digitalWrite(IN1, HIGH);
  delay(1000);
  digitalWrite(IN1, LOW);

  digitalWrite(IN2, HIGH);
  delay(1000);
  digitalWrite(IN2, LOW);

  digitalWrite(IN3, HIGH);
  delay(1000);
  digitalWrite(IN3, LOW);

  digitalWrite(IN4, HIGH);
  delay(1000);
  digitalWrite(IN4, LOW);

}